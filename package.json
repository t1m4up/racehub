  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "pnpm run dev",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset && npm run db:push && npm run db:seed",
